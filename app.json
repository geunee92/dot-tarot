        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDisplayName": "도트 타로",
        "NSUserTrackingUsageDescription": "이 앱은 맞춤형 광고를 제공하기 위해 기기 정보를 사용합니다. This app uses device information to provide personalized ads."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f0f23"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.geunee92.taroapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5957078713826547~2025849647",
          "iosAppId": "ca-app-pub-5957078713826547~2617882179"
        }
      ],
      "expo-tracking-transparency"
    ],
    "extra": {
      "eas": {
        "projectId": "552d60eb-d8f7-4a44-bea7-015ac799960d"
      }
    },
    "owner": "geunee92"
  }
